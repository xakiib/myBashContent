                      // D61 F86			
                      // EDD conducted By: Account Opening Officer 	
                      workbook.sheet("EDD").cell("D61").value(workbookdata[0].CUST_EDD.length>0? workbookdata[0].CUST_EDD[0].APPROVED_BY_NAME :"" );
                      // H61 F89
                      // Date:			
                      workbook.sheet("EDD").cell("H61").value(new Date(workbookdata[0].CUST_EDD[0].APPROVED_BY_DATE.toString()).toDateString("d-m-yy"));

                      // D62 F87			
                      // EDD conducted By: Account Opening Officer 	
                      workbook.sheet("EDD").cell("D62").value(workbookdata[0].CUST_EDD.length>0? workbookdata[0].CUST_EDD[0].APPROVED_COMPL_AUTH_NAME :"" );
                      // H62 F90
                      // Date:			
                      workbook.sheet("EDD").cell("H62").value(new Date(workbookdata[0].CUST_EDD[0].APPROVED_COMPL_AUTH_DATE.toString()).toDateString("d-m-yy"));

                      console.log("Exiting EDD");
                      //#endregion
                      
                    }
                   else {
                    workbook.deleteSheet("EDD")
                   }

                   if(workbookdata[0].CUST_CDD.length > 0){
                    console.log("Entering CDD-Individual");
                     //#region CDD-Individual
                     workbook.sheet("CDD-Individual").cell("E2").value(workbookdata[0].MYSIS_REF + " ("+workbookdata[0].TRACKING_ID+")");
                     // B3 OCAF22				
                     workbook.sheet("CDD-Individual").cell("B3").value(workbookdata[0].CUST_INFO[0].FIRST_NAME + " " + workbookdata[0].CUST_INFO[0].MIDDLE_NAME + " " + workbookdata[0].CUST_INFO[0].LAST_NAME);
                     // B4 ANCF03	
                     //Title of Account:			
                     workbook.sheet("CDD-Individual").cell("B4").value(workbookdata[0].NAME);
