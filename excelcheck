                        //Country Inward			
                        workbook.sheet("EDD").cell("C24").value(workbookdata[0].CUST_EDD[0].COUNTRY_DESC_INWARD_FOREIGN1);
                        // D24 F24			
                        workbook.sheet("EDD").cell("D24").value(workbookdata[0].CUST_EDD[0].COUNTRY_DESC_INWARD_FOREIGN2);
                        // F24 	F26		
                        workbook.sheet("EDD").cell("F24").value(workbookdata[0].CUST_EDD[0].COUNTRY_DESC_INWARD_FOREIGN3);
