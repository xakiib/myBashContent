                        workbook.sheet("EDD").cell("C26").value(workbookdata[0].CUST_EDD[0].AMOUNT_OF_FOREIGN_TRANSACTION);
                        // Purpose of transfer	
                        workbook.sheet("EDD").cell("F26").value(workbookdata[0].CUST_EDD[0].PURPOSE_OF_FOREIGN_TRANSACTION);

                        // F24 	F29		
                        workbook.sheet("EDD").cell("F26").value(workbookdata[0].CUST_EDD[0].PURPOSE_OF_FOREIGN_TRANSACTION);
                        //C27 F30			
                        //Name of beneficiaries of transfer	
                        workbook.sheet("EDD").cell("C27").value(workbookdata[0].CUST_EDD[0].NAME_OF_BENEFICIARY_FOREIGN_TRANSFER_1);
                        // D27 F31			
                        workbook.sheet("EDD").cell("D27").value(workbookdata[0].CUST_EDD[0].NAME_OF_BENEFICIARY_FOREIGN_TRANSFER_2);
                        // F27 	F32		
