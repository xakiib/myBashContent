                           workbook.sheet("AOINDVS").cell("D17").value(workbookdata[0].CUST_INFO.length>0 ?workbookdata[0].CUST_INFO[0].NATIONALITYDESC:"");
                           // L17 Other Nationalities	
                           if (workbookdata[0].CUST_INFO[0].DUAL_NATIONALITY == '0') { workbook.sheet("AOINDVS").cell("L17").value("No") }
                           else if (workbookdata[0].CUST_INFO[0].DUAL_NATIONALITY == '1'){ workbook.sheet("AOINDVS").cell("L17").value("Yes") };

                           // U17 If 'Yes' list nationlaties			
                           workbook.sheet("AOINDVS").cell("U17").value("");

                           // D18 City of Birth		
                           workbook.sheet("AOINDVS").cell("D18").value(workbookdata[0].CUST_INFO.length>0 ?workbookdata[0].CUST_INFO[0].CITYOFBIRTHDESC:"");
