                    for (var i = 0; i < workbookdata[0].CUST_JOINT.length; i++) {
                        var cellname;
                        if (i == 0) {  cellname = "C42" }
                        else if (i == 1) {  cellname = "C43" }
                        else {  cellname = "C44" }
