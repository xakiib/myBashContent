                            if (workbookdata[0].CUST_ACCOUNTS[0].ZAKAT_EXEMP == '0') { workbook.sheet("AOINDVS").cell("H61").value('No') }
                            else { workbook.sheet("AOINDVS").cell("H61").value('Yes') }

                            // U58 Free Insurance (For Rutba Accounts Only)
                            if (workbookdata[0].CUST_ACCOUNTS[0].ZAKAT_EXEMPT == '0') { workbook.sheet("AOINDVS").cell("U61").value('No') }
                            else { workbook.sheet("AOINDVS").cell("U61").value('Yes') };


                            // K59 HBL Nisa (for female account holders only)
                            if (workbookdata[0].CUST_ACCOUNTS[0].HBL_NISA == '0'){  workbook.sheet("AOINDVS").cell("K62").value("No");   }
                            else {workbook.sheet("AOINDVS").cell("K62").value("Yes");}
