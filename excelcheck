                            if (workbookdata[0].CUST_NEXTOFKIN.length > 0 ){ 
                            // C63 Name	
                            workbook.sheet("AOINDVS").cell("C64").value(workbookdata[0].CUST_NEXTOFKIN[0].NAME);
