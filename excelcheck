                    for (var i = 0; i < workbookdata[0].CUST_JOINT.length; i++) {
                        var cellname;
                        if (i == 0) {  cellname = "C33" }
                        else if (i == 1) {  cellname = "D33" }
                        else {  cellname = "E33" }

