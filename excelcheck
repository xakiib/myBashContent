                            workbook.sheet("CDD-Individual").cell("B5").value(workbookdata[0].CUST_INFO.length>0? workbookdata[0].CUST_INFO[0].CIF_NO:"");
                            // E5 0CAF00				
                            workbook.sheet("CDD-Individual").cell("E5").value(workbookdata[0].CUST_ACCOUNTS.length>0? workbookdata[0].CUST_ACCOUNTS[0].ACCT_NO:"");
                            // B6 CABRN				
                            workbook.sheet("CDD-Individual").cell("B6").value(_BRANCHNAME);
                            // E6 BR				
                            workbook.sheet("CDD-Individual").cell("E6").value(workbookdata[0].CUST_ACCOUNTS.length>0? workbookdata[0].CUST_ACCOUNTS[0].BRANCH_CODE:"");
                            // H6 AODT				
                            workbook.sheet("CDD-Individual").cell("H6").value(workbookdata[0].CUST_INFO.length>0? PROCESSING_DATE:"");
                            // B7 EA-INDIVIDUAL				
                            workbook.sheet("CDD-Individual").cell("B7").value(workbookdata[0].CUST_DEMOGRAPHICS.length>0? workbookdata[0].CUST_DEMOGRAPHICS[0].CUSTTYPEDESC:"");
                            // E7 				
                            workbook.sheet("CDD-Individual").cell("E7").value(workbookdata[0].CUST_DEMOGRAPHICS.length>0? workbookdata[0].CUST_DEMOGRAPHICS[0].OTERCUSTTYPEDESC:"");
                            // B8 INDVF11				
                            workbook.sheet("CDD-Individual").cell("B8").value(workbookdata[0].CUST_CDD.length>0? workbookdata[0].CUST_CDD[0].DESC_CUST_SRC_OF_INCOME_INDUS:"");
                            // E8  				
                            workbook.sheet("CDD-Individual").cell("E8").value(" ");
                            // B9 INDVF18				
                            workbook.sheet("CDD-Individual").cell("B9").value(ID_DOC_TYPE);
                            // E9 INDVF06				
                            workbook.sheet("CDD-Individual").cell("E9").value(workbookdata[0].CUST_INFO.length>0? workbookdata[0].CUST_INFO[0].ID_DOCUMENT_NO:"");
                            // H9 FACE TO FACE/ NON FACE TO FACE				
                            workbook.sheet("CDD-Individual").cell("H9").value(" ");
                            // B10 FOTNAT				
                            if (workbookdata[0].CUST_INFO[0].COUNTRY_OF_RES == 'PK'){
                                workbook.sheet("CDD-Individual").cell("B10").value("YES");
                            }else{
                                workbook.sheet("CDD-Individual").cell("B10").value("NO");
                            }
                            // D10 GFCNAP				
                            workbook.sheet("CDD-Individual").cell("D10").value(workbookdata[0].CUST_INFO.length>0? workbookdata[0].CUST_INFO[0].COUNTRYOFRESDESC:"");
                            // F10 INDVF18				
                            workbook.sheet("CDD-Individual").cell("F10").value(workbookdata[0].CUST_INFO.length>0? workbookdata[0].CUST_INFO[0].COUNTRYOFBIRTHDESC:"");
                            // C11 GFCNALP			
                            workbook.sheet("CDD-Individual").cell("C11").value(workbookdata[0].CUST_INFO.length>0? workbookdata[0].CUST_INFO[0].NATIONALITYDESC:"");
                            // C12 INDVF18				
                            workbook.sheet("CDD-Individual").cell("C12").value(workbookdata[0].CUST_INFO.length>0? workbookdata[0].CUST_INFO[0].SECONDNATIONALITYDESC:"");
                            // C13 				
                            workbook.sheet("CDD-Individual").cell("C13").value(" ");
                            // B14 CAAO09F				
                            workbook.sheet("CDD-Individual").cell("B14").value(workbookdata[0].CUST_CONTACTS.length>0? workbookdata[0].CUST_CONTACTS[0].MOBILE_NO:"");
                            // E14 CAAO12F				
                            workbook.sheet("CDD-Individual").cell("E14").value(workbookdata[0].CUST_CONTACTS.length>0? workbookdata[0].CUST_CONTACTS[0].EMAIL_ADDRESS:"");
                            // B15 CAAR01F				
                            workbook.sheet("CDD-Individual").cell("B15").value(workbookdata[0].CUST_ADDR[0].ADDRESS_LINE1 + " " + workbookdata[0].CUST_ADDR[0].ADDRESS_LINE2 + " " + workbookdata[0].CUST_ADDR[0].ADDRESS_LINE3 );
                            // B16 CAAP01F			
                            workbook.sheet("CDD-Individual").cell("B16").value(workbookdata[0].CUST_ADDR[1].ADDRESS_LINE1 + " " + workbookdata[0].CUST_ADDR[1].ADDRESS_LINE2 + " " + workbookdata[0].CUST_ADDR[1].ADDRESS_LINE3 );
                            // B17 CAAO01F				
                            workbook.sheet("CDD-Individual").cell("B17").value(workbookdata[0].CUST_ADDR[2].ADDRESS_LINE1 + " " + workbookdata[0].CUST_ADDR[2].ADDRESS_LINE2 + " " + workbookdata[0].CUST_ADDR[2].ADDRESS_LINE3 );
