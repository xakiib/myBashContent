                        workbook.deleteSheet("IL");
                        workbook.deleteSheet("IM");
                        workbook.deleteSheet("IP");
                        workbook.deleteSheet("IQ");
                        workbook.deleteSheet("IA");
                    }
                    else {
                        workbook.deleteSheet("IA");
                        workbook.deleteSheet("IB");
                        workbook.deleteSheet("IC");
                        workbook.deleteSheet("ID");
                        workbook.deleteSheet("IG");
                        workbook.deleteSheet("IJ");
                        workbook.deleteSheet("IK");
                        workbook.deleteSheet("IL");
                        workbook.deleteSheet("IM");
                        workbook.deleteSheet("IP");
                        workbook.deleteSheet("IQ");
                        workbook.deleteSheet("IS");
                    }
        //#endregion
                            
                    
                    workbook.toFileAsync("/home/aof_api/ExcelSheets/MINOR_CIF_ACCT_ISLAMIC_data.XLSX").then(function(){
                        exec('libreoffice --headless --convert-to pdf /home/aof_api/ExcelSheets/MINOR_CIF_ACCT_ISLAMIC_data.XLSX', (err, stdout, stderr) =>
                        {
                            if (err) {
                                // node couldn't execute the command
                                console.log(err);
                                return;
                            }
                            else {
                                var data = fs.readFileSync('/home/aof_api/MINOR_CIF_ACCT_ISLAMIC_data.pdf');
                                res.contentType("application/pdf");
                                res.send(data);
                                exec('rm /home/aof_api/ExcelSheets/MINOR_CIF_ACCT_ISLAMIC_data.XLSX', (err, stdout, stderr) =>
                                {
                                    if (err) {console.log('file deletion error', err);return;}
                                    res.end();
                                });
                            }
                    });
                  
                    }, (error) => { console.log(error) });

                })
            }


