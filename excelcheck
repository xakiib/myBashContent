                           workbook.sheet("AOINDVS").cell("T18").value(workbookdata[0].CUST_INFO.length>0 ?workbookdata[0].CUST_INFO[0].COUNTRYOFRESDESC:"");


                           // S19 Are you, or have you ever been a citizen or tax resident of a country other than Pakistan?
                           if (workbookdata[0].CUST_INFO[0].TAX_PAYER_OTHER_COUNTRY == '0') { 
                               workbook.sheet("AOINDVS").cell("S19").value('No'); 
                            }
                           else if (workbookdata[0].CUST_INFO[0].TAX_PAYER_OTHER_COUNTRY == '1') { 
                               workbook.sheet("AOINDVS").cell("S19").value('Yes'); 
                            }else{
                                workbook.sheet("AOINDVS").cell("S19").value(""); 
                            }


                          
