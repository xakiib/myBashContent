                    workbook.toFileAsync(excelLocation+'AOMINORJOINT_ISLAMIC_data.xlsx').then(function(){
                        exec('libreoffice --headless --convert-to pdf /home/aof_api/ExcelSheets/AOMINORJOINT_ISLAMIC_data.xlsx', (err, stdout, stderr) =>
                        {
                            if (err) {
                                // node couldn't execute the command
                                console.log(err);
                                return;
                            }
                            else {
                                var data = fs.readFileSync('/home/aof_api/AOMINORJOINT_ISLAMIC_data.pdf');
                                res.contentType("application/pdf");
                                // res.send("foo");
                                res.send(data);
    
                                exec('rm /home/aof_api/ExcelSheets/AOMINORJOINT_ISLAMIC_data.xlsx', (err, stdout, stderr) =>
                                {
                                    if (err) {console.log('file deletion error', err);return;}
                                    res.end();
                                });
                            }
                        });
                    });


                   
                })
            }


            //  CIF_ACC
            else if (jointvalue == 1 && workflow == 3 && accountType.Convential_Islamic == 'C' ) {
                console.log("WORKING FILE NAME: CIF_ACC.xlsx");    
                    XlsxPopulate.fromFileAsync(excelLocation+'CIF_ACC.xlsx')
                        .then(async workbook =>
                        {
                            console.log("Entering AOINDVS");
                            //#region AOINDVS
                            workbook.sheet("AOINDVS").cell("R2").value(workbookdata[0].MYSIS_REF + " ("+workbookdata[0].TRACKING_ID+")");

