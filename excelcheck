                    workbook.sheet("ARF").cell("F21").value(workbookdata[0].CUST_INFO.length>0 ?workbookdata[0].CUST_INFO[0].COUNTRYOFRESDESC :"" );
                    // B22 						
                    workbook.sheet("ARF").cell("B22").value(workbookdata[0].CUST_ADDR.length>0 ?workbookdata[0].CUST_ADDR[0].NEAREST_LANDMARK :"" );

                    // F22					
                    workbook.sheet("ARF").cell("F22").value(workbookdata[0].CUST_ADDR.length>0 ?workbookdata[0].CUST_ADDR[0].POSTCODE :"" );
                    // B23 						
                    workbook.sheet("ARF").cell("B23").value(workbookdata[0].CUST_INFO.length>0 ?workbookdata[0].CUST_INFO[0].COUNTRYOFRESDESC :"" );
                    // D23					
                    workbook.sheet("ARF").cell("D23").value(workbookdata[0].CUST_INFO.length>0 ?workbookdata[0].CUST_INFO[0].CITYOFBIRTHDESC :"" );
                    // F23					
                    workbook.sheet("ARF").cell("F23").value(workbookdata[0].CUST_INFO.length>0 ?workbookdata[0].CUST_INFO[0].COUNTRYOFBIRTHDESC :"" );
