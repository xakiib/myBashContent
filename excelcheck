                    workbook.sheet("ARF").cell("E39").value(workbookdata[0].CUST_ACCOUNTS.length>0? workbookdata[0].CUST_ACCOUNTS[0].NAME_ON_CARD:"");

                    // HBL DEBIT CARD REQUEST //

                    // C41					
