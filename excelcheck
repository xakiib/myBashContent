                        console.log("Exiting EDD");
                       
                        
                    }
                   else {
                    workbook.deleteSheet("EDD")
                   }
                    //#endregion
                   
                     //#region CDD

                    if(workbookdata[0].CUST_CDD.length > 0){
                    console.log("Entering CDD-Individual");

                            // Modify the workbook.
                            workbook.sheet("CDD-Individual").cell("F2").value(workbookdata[0].MYSIS_REF + " ("+workbookdata[0].TRACKING_ID+")");
                            // B3 OCAF22				
                            workbook.sheet("CDD-Individual").cell("B3").value(workbookdata[0].CUST_INFO[0].FIRST_NAME + " " + workbookdata[0].CUST_INFO[0].MIDDLE_NAME + " " + workbookdata[0].CUST_INFO[0].LAST_NAME);
                            // B4 ANCF03				
                            workbook.sheet("CDD-Individual").cell("B4").value(workbookdata[0].CUST_ACCOUNTS.length>0? workbookdata[0].CUST_ACCOUNTS[0].TITLE_OF_ACCT:"");
