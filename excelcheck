                    // workbook.sheet("ARF").cell("F25").value(workbookdata[0].CUST_INFO[0].SECOND_NATIONALITY == 'US' ? 'YES' : 'NO');
                    if (workbookdata[0].CUST_INFO[0].TAX_PAYER_OTHER_COUNTRY == '0' || workbookdata[0].CUST_INFO[0].TAX_PAYER_OTHER_COUNTRY == '0' ) { 
                        workbook.sheet("ARF").cell("F25").value('No'); 
                    }
                    else  if (workbookdata[0].CUST_INFO[0].TAX_PAYER_OTHER_COUNTRY == '1' || workbookdata[0].CUST_INFO[0].TAX_PAYER_OTHER_COUNTRY == '1' ) { 
                             workbook.sheet("ARF").cell("F25").value('Yes'); 
                    }

