                    // workbook.sheet("ARF").cell("C24").value(workbookdata[0].CUST_INFO.length>0 ?workbookdata[0].CUST_INFO[0].DUAL_NATIONALITY :"" );
                    workbook.sheet("ARF").cell("F15").value(workbookdata[0].CUST_INFO[0].PEP = 0? "YES" :"NO" );
