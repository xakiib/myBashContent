                        workbook.deleteSheet("ED");
                        workbook.deleteSheet("EH");
                        workbook.deleteSheet("EZ");
                        workbook.deleteSheet("GE");
                        workbook.deleteSheet("GM");
                        workbook.deleteSheet("EN");
                        workbook.deleteSheet("GN");
                    }
                    else if (acctype == 'EN') {
                        workbook.deleteSheet("C4");
                        workbook.deleteSheet("C7");
                        workbook.deleteSheet("CA");
                        workbook.deleteSheet("CD");
                        workbook.deleteSheet("EA");
                        workbook.deleteSheet("EC");
                        workbook.deleteSheet("ED");
                        workbook.deleteSheet("EH");
                        workbook.deleteSheet("EZ");
                        workbook.deleteSheet("GE");
