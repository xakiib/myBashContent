                        // H57 FPEP			
                        // Has financial Supportor / Guardian or his/ her immediate family member / close associate is / ever been a politically exposed person?
                        if (workbookdata[0].CUST_EDD[0].FIN_SUPP_GUR_FMLY_PEP == '0') { workbook.sheet("EDD").cell("H57").value('No') }
                        else { workbook.sheet("EDD").cell("H57").value('Yes') }


                        //    VERIFICATION      //

                        // D60 F85			
