                    //#endregion
                            
                            if (pepvalue != 1) {

                                workbook.deleteSheet("PEP")
                                // workbook.sheet("PEP").delete();
                            }
                            // console.log("exiting");
                            // workbook.toFileAsync(excelLocation+ 'CIF_ACC_data.xlsx');
                            workbook.toFileAsync('/home/aof_api/ExcelSheets/CIF_ACC_data.xlsx').then(function(){
                                exec('libreoffice --headless --convert-to pdf /home/aof_api/ExcelSheets/CIF_ACC_data.xlsx', (err, stdout, stderr) =>
                                {
                                    if (err) {
                                        // node couldn't execute the command
                                        console.log(err);
                                        return;
                                    }
                                    else {
                                        var data = fs.readFileSync('/home/aof_api/CIF_ACC_data.pdf');
                                        res.contentType("application/pdf");
                                        res.send(data);
                                        
                                        exec('rm /home/aof_api/ExcelSheets/CIF_ACC_data.xlsx', (err, stdout, stderr) =>
                                        {
                                            if (err) {console.log('file deletion error', err);return;}
                                            res.end();
                                         });
                                    }
                                });
                            });
                           
                        }, (error) => { console.log(error) })


            }

             //  CIF_ACC_ISLAMIC
             else if (jointvalue == 1 && workflow == 3 && accountType.Convential_Islamic == 'I' ) {
                console.log("WORKING FILE NAME: CIF_ACC_ISLAMIC.xlsx");    
