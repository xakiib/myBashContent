            //#endregion AMMENDMENT

            
            function getIDDocType(getIDDocType){
                return new Promise((resolve, reject)=>{
                    try {
                        if(getAccountType != "" && getAccountType != '' && getAccountType != undefined ){
                            db.collection('AOF_LOV_ID_DOC_TYPE').find({ID_DOC_CODE: getIDDocType}).toArray(async function (err, ID_DOC_CODE){
                                // console.log('ID_DOC_CODE');
                                console.log(ID_DOC_CODE);
                                resolve (ID_DOC_CODE[0].ID_DOC_DESC)
                            });   
                        }
                        
                    } catch (error) {
                        
                    }
                });
                
            }

            function getBranchName(BRANCH_CODE){

                return new Promise((resolve, reject)=>{
                    try {
                        db.collection('AOF_LOV_BRANCH_CODE').find({BR_CODE: BRANCH_CODE}).toArray(async function (err, BRANCH_DATA){
                            // console.log('ID_DOC_CODE');
                            // console.log(ID_DOC_CODE);
                            resolve (BRANCH_DATA[0].BR_NAME)
                        });
                    } catch (error) {
                        
                    }
                });
                
            }

            function getIndustryType(INDUSTRY_CODE){

                return new Promise((resolve, reject)=>{
                    try {
                        db.collection('AOF_LOV_SOURCE_OF_INCOME_INDUS').find({SOII_CODE: INDUSTRY_CODE}).toArray(async function (err, INDUSTRY_CODE_DATA){
                            // console.log('ID_DOC_CODE');
                            // console.log(ID_DOC_CODE);
                            resolve (INDUSTRY_CODE_DATA[0].SOII_DESC)
                        });
                    } catch (error) {
                        
                    }
                });
                
            }

            function getEconomicActivity(ACTIVITY_CODE){
                return new Promise((resolve, reject)=>{
                    try {
                        db.collection('AOF_LOV_ECONOMIC_ACTIVITY').find({FCOD: ACTIVITY_CODE}).toArray(async function (err, ACTIVITY_CODE_DATA){
                            // console.log('ID_DOC_CODE');
                            // console.log(ID_DOC_CODE);
                            resolve (ACTIVITY_CODE_DATA[0].FDES)
                        });
                    } catch (error) {
                        
                    }
                });
                
            }

            function getAccountType(CUST_SEGMENT, ACCT_TYPE){
                return new Promise((resolve, reject)=>{
                    try {
                        db.collection('AOF_LOV_ACCT_TYPE').find({CUST_SEGMENT:CUST_SEGMENT, ACCT_TYPE:ACCT_TYPE }).toArray(async function (err, AOF_LOV_ACCT_TYPE){
                            // console.log('ID_DOC_CODE');
                            // console.log(ID_DOC_CODE);
                            resolve (AOF_LOV_ACCT_TYPE[0])
                        });
                    } catch (error) {
                        
                    }
                });
                
            }

            function getReasonTIN_PURP(REASON_IN_CASE_OF_NO_TIN){
                return new Promise((resolve, reject)=>{
                    try {
                        if(REASON_IN_CASE_OF_NO_TIN != '' && REASON_IN_CASE_OF_NO_TIN != "" && REASON_IN_CASE_OF_NO_TIN != undefined){
                            db.collection('AOF_LOV_TIN_PURP').find({PURP_CODE :REASON_IN_CASE_OF_NO_TIN}).toArray(async function (err, PURP_OBJECT){
                                // console.log('ID_DOC_CODE');
                                // console.log(PURP_OBJECT[0].PURP_DESC);
                                resolve (PURP_OBJECT[0].PURP_DESC)
                            });
                        }else {
                            console.log("else getReasonTIN_PURP" );
                            resolve("");
                        }
                        
                    } catch (error) {
                        console.log(error.message);
                    }
                });
                
            }

            function getFormattedDate(DateString){
                if(DateString != ''){
                    var today = new Date(DateString);
                    var dd = String(today.getDate()).padStart(2, '0');
                    var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
                    var yyyy = today.getFullYear();

                    today = dd + '-' + mm + '-' + yyyy;
                    // document.write(today);
                    console.log(today);
                    return today;
                    // return new Date (DateString).toISOString().slice(0,10)
                }
                else {
                    return '';
                }
                
            }


        
            });
    });
});



module.exports = router;
