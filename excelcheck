                                {
                                    if (err) {
                                        // node couldn't execute the command
                                        console.log(err);
                                        return;
                                    }
                                    else {
                                        var data = fs.readFileSync('/home/aof_api/NON_INDIVIDUAL_data.pdf');
                                        res.contentType("application/pdf");
                                        res.send(data);
                                        exec('rm /home/aof_api/ExcelSheets/NON_INDIVIDUAL_data.XLSX', (err, stdout, stderr) =>
                                        {
                                            if (err) {console.log('file deletion error', err);return;}
                                            res.end();
                                        });
                                    }
                            });
                           
                            }, (error) => { console.log(error) });
                            
    
                        })
