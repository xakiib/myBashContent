                                    workbook.deleteSheet("IS");
                                }
                                else if (acctype == 'IS') {
                                    workbook.deleteSheet("IB");
                                    workbook.deleteSheet("IC");
                                    workbook.deleteSheet("ID");
                                    workbook.deleteSheet("IG");
                                    workbook.deleteSheet("IJ");
                                    workbook.deleteSheet("IK");
                                    workbook.deleteSheet("IL");
                                    workbook.deleteSheet("IM");
                                    workbook.deleteSheet("IP");
                                    workbook.deleteSheet("IQ");
                                    workbook.deleteSheet("IA");
                                }
                                else {
                                    workbook.deleteSheet("IA");
                                    workbook.deleteSheet("IB");
                                    workbook.deleteSheet("IC");
                                    workbook.deleteSheet("ID");
                                    workbook.deleteSheet("IG");
                                    workbook.deleteSheet("IJ");
                                    workbook.deleteSheet("IK");
                                    workbook.deleteSheet("IL");
                                    workbook.deleteSheet("IM");
                                    workbook.deleteSheet("IP");
                                    workbook.deleteSheet("IQ");
                                    workbook.deleteSheet("IS");
                                }
                    //#endregion
                            
                            if (pepvalue != 1) {

                                workbook.deleteSheet("PEP")
                                // workbook.sheet("PEP").delete();
                            }
                            // console.log("exiting");
                            // workbook.toFileAsync(excelLocation+ 'CIF_ACC_data.xlsx');
                            workbook.toFileAsync(excelLocation+'CIF_ACC_ISLAMIC_data.xlsx').then(function(){
                                exec('libreoffice --headless --convert-to pdf /home/aof_api/ExcelSheets/CIF_ACC_ISLAMIC_data.xlsx', (err, stdout, stderr) =>
                                {
                                    if (err) {
                                        // node couldn't execute the command
                                        console.log(err);
                                        return;
                                    }
                                    else {
                                        var data = fs.readFileSync('/home/aof_api/CIF_ACC_ISLAMIC_data.pdf');
                                        res.contentType("application/pdf");
                                        res.send(data);
                                        
                                        exec('rm /home/aof_api/ExcelSheets/CIF_ACC_ISLAMIC_data.xlsx', (err, stdout, stderr) =>
                                        {
                                            if (err) {console.log('file deletion error', err);return;}
                                            res.end();
                                         });
                                    }
                                });
                            });
                           
                        }, (error) => { console.log(error) })


            }


            // AOINDVS
            else if (jointvalue == 1 && (workflow == 2 || workflow == 5) && accountType.Convential_Islamic == 'C' ) {
