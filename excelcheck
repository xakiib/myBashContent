                                else {
                                    console.log(data);
                                    exec('libreoffice --headless --convert-to pdf /home/aof_api/ExcelSheets/AOINDVS_ISLAMIC_data.xlsx', (err, stdout, stderr) =>
                                {
                                    console.log("libreoffice");
                                    if (err) {
                                        // node couldn't execute the command
                                        console.log(err);
                                        return;
                                    }
                                    else {
                                        console.log("converting to pdf");
                                        var data = fs.readFileSync('/home/aof_api/AOINDVS_ISLAMIC_data.pdf');
                                        exec('rm /home/aof_api/ExcelSheets/AOINDVS_ISLAMIC_data.xlsx', (err, stdout, stderr) =>
                                        {
                                            console.log("removing excel");
                                            if (err) {console.log('file deletion error', err);return;}
                                            // res.end();
                                        });
                                        res.contentType("application/pdf");

                                        res.send(data);

                                        exec('rm /home/aof_api/AOINDVS_ISLAMIC_data.pdf', (err, stdout, stderr) =>
                                        {
                                            console.log("removing pdf");
                                            if (err) {console.log('file deletion error', err);return;}
                                            res.end();
                                        });
    
                                        
                                       
                                    }
                                });
                                }
                            });


