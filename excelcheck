                        // D13 F09			
                        if(workbookdata[0].CUST_EDD[0].AGRI_RENTAL_INCOME == "1"){workbook.sheet("EDD").cell("D13").value("Yes");}
                        else{workbook.sheet("EDD").cell("D13").value("No");}
                        // workbook.sheet("EDD").cell("D13").value(workbookdata[0].CUST_EDD[0].AGRI_RENTAL_INCOME);
                        // G13 F14			
                        workbook.sheet("EDD").cell("G13").value(workbookdata[0].CUST_EDD[0].AGRI_RENTAL_INCOME_EXP_AMT);
                        // D14 F10		
                        if(workbookdata[0].CUST_EDD[0].BUSINESS_INCOME == "1"){workbook.sheet("EDD").cell("D14").value("Yes");}
                        else{workbook.sheet("EDD").cell("D14").value("No");}
                        // workbook.sheet("EDD").cell("D14").value(workbookdata[0].CUST_EDD[0].BUSINESS_INCOME);
                        // G14 F15			
                        workbook.sheet("EDD").cell("G14").value(workbookdata[0].CUST_EDD[0].BUSINESS_INCOME_EXP_AMT);
                        // D15 F11			
                        if(workbookdata[0].CUST_EDD[0].RENTAL_INCOME == "1"){workbook.sheet("EDD").cell("D15").value("Yes");}
                        else{workbook.sheet("EDD").cell("D15").value("No");}
                        // workbook.sheet("EDD").cell("D15").value(workbookdata[0].CUST_EDD[0].RENTAL_INCOME);
                        // G15 F16			
                        workbook.sheet("EDD").cell("G15").value(workbookdata[0].CUST_EDD[0].RENTAL_INCOME_EXP_AMT);
                        // D16 F12			
                        if(workbookdata[0].CUST_EDD[0].RENTAL_INCOME_RES_PROJ == "1"){workbook.sheet("EDD").cell("D16").value("Yes");}
                        else{workbook.sheet("EDD").cell("D16").value("No");}
                        // workbook.sheet("EDD").cell("D16").value(workbookdata[0].CUST_EDD[0].RENTAL_INCOME_RES_PROJ);
                        // G16 F17			
                        workbook.sheet("EDD").cell("G16").value(workbookdata[0].CUST_EDD[0].RENTAL_INCOME_RES_PROJ_EXP_AMT);
                        // D17 No			
                        if(workbookdata[0].CUST_EDD[0].INHERITANCE == "1"){workbook.sheet("EDD").cell("D17").value("Yes");}
                        else{workbook.sheet("EDD").cell("D17").value("No");}
                        // workbook.sheet("EDD").cell("D17").value(workbookdata[0].CUST_EDD[0].INHERITANCE);
                        // G17 0			
                        workbook.sheet("EDD").cell("G17").value(workbookdata[0].CUST_EDD[0].INHERITANCE_EXP_AMT);
