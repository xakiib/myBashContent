                        if(workbookdata[0].CUST_EDD[0].CONDUCT_FOREIGN_BANK_TRANSFER == "1"){workbook.sheet("EDD").cell("H22").value("Yes");}
                        else{workbook.sheet("EDD").cell("H22").value("No");}					
