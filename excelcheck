                    // B21 						
                    workbook.sheet("ARF").cell("B21").value(workbookdata[0].CUST_ADDR.length>0 ?workbookdata[0].CUST_ADDR[0].ADDRESS_LINE3 :"" );
                    // D21					
                    workbook.sheet("ARF").cell("D21").value(workbookdata[0].CUST_ADDR.length>0 ?workbookdata[0].CUST_ADDR[0].CITY_NAME :"" );
